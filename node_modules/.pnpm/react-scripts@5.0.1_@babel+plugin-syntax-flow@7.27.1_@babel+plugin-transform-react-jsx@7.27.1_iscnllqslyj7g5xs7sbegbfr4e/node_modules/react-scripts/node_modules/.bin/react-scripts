#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/dev/star_share/web/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_iscnllqslyj7g5xs7sbegbfr4e/node_modules/react-scripts/bin/node_modules:/opt/dev/star_share/web/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_iscnllqslyj7g5xs7sbegbfr4e/node_modules/react-scripts/node_modules:/opt/dev/star_share/web/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_iscnllqslyj7g5xs7sbegbfr4e/node_modules:/opt/dev/star_share/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/opt/dev/star_share/web/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_iscnllqslyj7g5xs7sbegbfr4e/node_modules/react-scripts/bin/node_modules:/opt/dev/star_share/web/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_iscnllqslyj7g5xs7sbegbfr4e/node_modules/react-scripts/node_modules:/opt/dev/star_share/web/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_iscnllqslyj7g5xs7sbegbfr4e/node_modules:/opt/dev/star_share/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../../bin/react-scripts.js" "$@"
fi
